syntax enable
colorscheme kolor
set tags=tags;/
set t_Co=256
set nocompatible
syntax on
"set enc=gb2312
set enc=utf-8
set fileencodings=utf-8
"set fileencodings=utf-8,gbk
set termencoding=utf-8
"set tenc=gbk
set nobackup
set encoding=utf-8
set autoindent
set cindent
set shiftwidth=4
set tabstop=4
set expandtab
set softtabstop=4
"set paste
set listchars=tab:->,trail:-
"set list
set fileformats=unix

"set fdm=marker
"set foldmethod=indent

set backspace=indent,eol,start
set history=50
set showcmd
set number
set incsearch
"set ignorecase
set hlsearch
"set mouse=a
set wildmenu
filetype plugin indent on
set cino=:0,l1,g0,p0,t0

"set tenc=gbk
